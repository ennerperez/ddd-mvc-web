<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup Label="General">
		<UsingAssemblyInfoTemplate>false</UsingAssemblyInfoTemplate>
	</PropertyGroup>

	<PropertyGroup Label="Databases">
		<UsingSqlite>true</UsingSqlite>
		<UsingSqlServer>true</UsingSqlServer>
		<UsingMariaDb>true</UsingMariaDb>
		<UsingMySql>true</UsingMySql>
		<UsingPostgreSql>true</UsingPostgreSql>
		<UsingOracle>true</UsingOracle>
	</PropertyGroup>

	<PropertyGroup Label="Azure">
		<UsingAzureApplicationInsights>true</UsingAzureApplicationInsights>
		<UsingAzureBlobs>true</UsingAzureBlobs>
		<UsingAzureQueues>true</UsingAzureQueues>
		<UsingAzureTables>true</UsingAzureTables>
		<UsingAzureKeyVault>true</UsingAzureKeyVault>
	</PropertyGroup>

	<PropertyGroup Label="Services">
		<UsingQuestPDF>true</UsingQuestPDF>
		<UsingNewtonsoft>true</UsingNewtonsoft>
		<UsingSwagger>true</UsingSwagger>
		<UsingLocalization>true</UsingLocalization>
		<UsingSass>true</UsingSass>
	</PropertyGroup>

	<PropertyGroup Label="Auth">
		<UsingCookies>true</UsingCookies>
		<UsingApiKey>true</UsingApiKey>
		<UsingBearer>true</UsingBearer>
		<UsingOpenId>true</UsingOpenId>
		<UsingAuth0>true</UsingAuth0>
	</PropertyGroup>

	<PropertyGroup Label="Options">
		<EnableNonAsync>true</EnableNonAsync>
		<!--<EnableBulk>true</EnableBulk>-->
		<EnableSplit>true</EnableSplit>
		<EnableSassWatch>true</EnableSassWatch>
		<EnableDatadog>true</EnableDatadog>
		<EnableNewRelic>true</EnableNewRelic>
		<EnableSmartSchema>true</EnableSmartSchema>
		<EnableApiKeyTables>true</EnableApiKeyTables>
		<EnableTokenValidation>true</EnableTokenValidation>
	</PropertyGroup>

	<PropertyGroup Label="Testing">
		<TestExecutionFramework>xUnit</TestExecutionFramework>
		<DefaultLanguage>EN</DefaultLanguage>
		<EnableChromeDriver>true</EnableChromeDriver>
		<EnableGeckoDriver>true</EnableGeckoDriver>
		<EnableIEDriver>true</EnableIEDriver>
	</PropertyGroup>

</Project>