<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup Label="Databases">
		<UsingSqlite>true</UsingSqlite>
		<UsingSqlServer>true</UsingSqlServer>
		<UsingMariaDb>true</UsingMariaDb>
		<UsingMySql>true</UsingMySql>
		<UsingPostgreSql>true</UsingPostgreSql>
		<UsingOracle>true</UsingOracle>
	</PropertyGroup>

	<ItemGroup Label="Contexts">
		<Context Include="DefaultContext" Engine="SQLITE" />
	</ItemGroup>

	<PropertyGroup Label="Azure">
		<UsingAzureBlobs>false</UsingAzureBlobs>
		<UsingAzureQueues>false</UsingAzureQueues>
		<UsingAzureTables>false</UsingAzureTables>
		<UsingAzureKeyVault>false</UsingAzureKeyVault>
	</PropertyGroup>

	<PropertyGroup Label="Services">
		<UsingQuestPDF>true</UsingQuestPDF>
		<UsingNewtonsoft>true</UsingNewtonsoft>
		<UsingSwagger>true</UsingSwagger>
		<UsingLocalization>true</UsingLocalization>
		<UsingInsights>true</UsingInsights>
		<UsingSass>true</UsingSass>
		<UsingSmartSchema>true</UsingSmartSchema>
	</PropertyGroup>

	<PropertyGroup Label="Options">
		<EnableNonAsync>true</EnableNonAsync>
		<EnableBulk>true</EnableBulk>
		<EnableSplit>true</EnableSplit>
		<EnableSassWatch>true</EnableSassWatch>
		<EnableBearer>true</EnableBearer>
		<EnableApiKey>true</EnableApiKey>
		<EnableOpenId>false</EnableOpenId>
		<EnableAB2C>false</EnableAB2C>
		<EnableLocalAB2C>false</EnableLocalAB2C>
	</PropertyGroup>

	<PropertyGroup Label="Testing">
		<TestExecutionFramework>xUnit</TestExecutionFramework>
		<DefaultLanguage>EN</DefaultLanguage>
	</PropertyGroup>

</Project>