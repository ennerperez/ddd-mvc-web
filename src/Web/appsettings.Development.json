{
  "AppSettings": {
    "CacheControl": 86400
  },
  "SwaggerSettings": {
    "UIEngine": "SwaggerUI"
  },
  "AzureSettings": {
    "ApplicationInsights": {
      "ConnectionString": ""
    }
  },
  "GoogleSettings": {
    "Analytics": {
      "TrackingCode": ""
    }
  },
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}