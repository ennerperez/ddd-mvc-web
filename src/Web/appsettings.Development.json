{
	"AppSettings": {
		"CacheControl": 86400
	},
	"AuthSettings": {
		"Authority": "https://localhost:5001",
		"RequireHttpsMetadata": false
	},
	"SwaggerSettings": {
		"UIEngine": "SwaggerUI"
	},
	"AzureSettings": {
		"ApplicationInsights": {
			"ConnectionString": null
		},
		"Storage": {
			"ConnectionString": "UseDevelopmentStorage=true"
		}
	},
	"GoogleSettings": {
		"Analytics": {
			"TrackingCode": null
		},
		"TagManager": {
			"ContainerId": null
		},
		"Maps": {
			"Key": null
		}
	},
	"DetailedErrors": true,
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"Microsoft": "Information",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Information",
				"System": "Information"
			}
		},
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "File",
				"Args": {
					"path": "Logs/.log",
					"rollingInterval": "Day",
					"flushToDiskInterval": 30,
					"shared": true
				}
			}
		]
	}
}