{
  "ConnectionStrings": {
    "CacheContext_Sqlite": "Data Source=cache_test.db;Cache=Shared",
    "DefaultContext_SqlServer": "Data Source=(LocalDb)\\MSSQLLocalDB;Initial Catalog=database_test;Integrated Security=true;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/.log",
          "rollingInterval": "Day",
          "flushToDiskInterval": 30
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithThreadName"
    ]
  }
}